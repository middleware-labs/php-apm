{
    "name": "middleware/php-apm-test",
    "description": "PHP APM Test Package",
    "keywords": ["apm"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sanjay Suthar",
            "email": "sanjay@middleware.io",
            "homepage": "https://middleware.io/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "open-telemetry/sdk": "^1.0.0beta3",
        "open-telemetry/api": "1.0.0beta4",
        "open-telemetry/context": "1.0.0beta2",
        "open-telemetry/sem-conv": "1.0.0beta2",
        "symfony/http-client": "6.3.x-dev",
        "guzzlehttp/promises": "^1.5@dev",
        "php-http/message-factory": "^1.0@dev",
        "nyholm/psr7": "^1.5",
        "psr/http-client": "1.0.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Middleware\\PhpApmTest\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
